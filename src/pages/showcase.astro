            <HeroShowcase />

            <div id="main-content">
              <div id="main-page-content">
                <div id="itemsWrapperLinks">
                  <div id="itemsWrapper" class="webgl-fitthumbs fx-seven">
                    <div class="showcase-portfolio layout-one">
                      <div class="clapat-item photo-filter">
                        <div
                          class="slide-inner trigger-item"
                          data-centerLine="OPEN"
                        >
                          <div class="img-mask">
                            <a
                              class="slide-link"
                              data-type="page-transition"
                              href="/project01">
                            <div class="section-image trigger-item-link">
                              <img
                                src="/images/01hero.jpg"
                                class="item-image grid__item-img"
                                alt=""
                              />
                              
